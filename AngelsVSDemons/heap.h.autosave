#ifndef HEAP_H
#define HEAP_H
#include "person.h"
template <typename T>
struct NodeH{
  //Attr.
  NodeH *leftSon,*rightSon;
  T* data;
  //Constr.
  NodeH(T* data){
      leftSon = rightSon = nullptr;
      this->data = data;
  }
};
template <typename T>
struct Heap{
    //Attr.
    NodeH<T> *root;
    //Constr.
    Heap(){
        root = nullptr;
    }
    
};

#endif // HEAP_H
